{
    "name" : "default-value/akeneo-inline-edit-bundle",
    "type" : "symfony-bundle",
    "description" : "Add ability to enable inline edit for Akeneo PIM product grid",
    "keywords" : [
        "Akeneo",
        "PIM",
        "bundle",
        "Symfony2",
        "inline-edit"
    ],
    "homepage": "https://github.com/DefaultValue",
    "license": "MIT",
    "authors" : [
        {
            "name": "DefaultValue",
            "homepage": "http://default-value.com"
        }
    ],
    "require" : {
        "php": ">=5.4.4",
        "akeneo/pim-community-dev": "1.4.x-dev@dev"
    },
    "require-dev": {
        "doctrine/migrations": "1.0.0-alpha3@alpha",
        "doctrine/doctrine-migrations-bundle": "1.0.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/akeneo/pim-community-dev.git",
            "branch": "master"
        }
    ],
    "autoload" : {
        "psr-4" : { "DefaultValue\\Bundle\\AkeneoInlineEditBundle\\" : "" }
    },
    "minimum-stability": "stable"
}
